// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+space ctrl+r",
        "command": "C_Cpp.BuildAndRunFile"
    },
    {
        "key": "alt+`",
        "command": "workbench.action.terminal.new",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "-workbench.action.terminal.new",
        "when": "!terminalFocus"
    }
]